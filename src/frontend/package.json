{
  "name": "@repo/frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8000",
    "build": "NODE_ENV=production next build",
    "analyze": "ANALYSE=true next build",
    "start": "next start",
    "lint": "tsc --noEmit",
    "lint:watch": "tsc --noEmit --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage && open coverage/index.html"
  },
  "dependencies": {
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.3",
    "@types/node": "20.10.1",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.31",
    "typescript": "5.3.2",
    "vitest": "^0.34.6"
  }
}
